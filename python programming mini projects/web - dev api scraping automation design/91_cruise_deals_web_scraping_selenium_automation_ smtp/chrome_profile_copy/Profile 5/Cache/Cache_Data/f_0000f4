-options';\nimport { InputsSliderRange } from '@y2/feed-search/components/composed/inputs-slider-range';\n\nimport styles from './squaremeter-range-input.module.scss';\nimport texts from './squaremeter-range-input.texts.json';\n\ntype Props = {\n  sliderOptions: RangeOptions;\n  value: Range;\n  onChange: Dispatch<SetStateAction<Range>>;\n};\n\nexport const SquareMeterRangeInput = ({\n  sliderOptions: { from, to, step },\n  value,\n  onChange,\n}: Props) => {\n  return (\n    <fieldset data-testid=\"squaremeter-range-input\">\n      <h2 className={styles.title}>{texts.title}</h2>\n      <InputsSliderRange\n        value={value}\n        setRangeValue={onChange}\n        limits={[from, to]}\n        steps={step}\n        data-testid=\"realestate-squaremeter-range\"\n      />\n    </fieldset>\n  );\n};\n","import { useRouter } from 'next/router';\nimport { encode } from 'querystring';\nimport { useRouterHash } from '@y2/hooks/use-router-hash';\n\ntype UseFullPathOptions = {\n  queryOverride?: Record<string, string>;\n};\n\nexport const useFullPath = (path: string, options: UseFullPathOptions) => {\n  const { query } = useRouter();\n  const hash = useRouterHash();\n\n  const encodedParams = encode(options?.queryOverride ?? query);\n  const separator = path.includes('?') ? '&' : '?';\n\n  return [\n    path,\n    encodedParams.length > 0 ? separator : '',\n    encodedParams,\n    hash && `#${hash}`,\n  ].join('');\n};\n","import Link from 'next/link';\nimport cn from 'classnames';\nimport { ResponsiveDropdown } from '@y2/feed-search/components/composed/responsive-dropdown';\nimport { InputTitle } from '@y2/feed-search/components/ui/input-title';\nimport { RoundedPseudoDropButton } from '@y2/feed-search/components/ui/pseudo-drop-button';\nimport { useCollapse } from '@y2/hooks/use-collapse';\nimport { CheckIcon } from '@y2/mango/components/icons';\n\nimport texts from './texts.json';\nimport styles from './transaction-type.module.scss';\nimport { useFullPath } from '../.