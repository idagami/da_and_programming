/fail';\n\ntype PaymentType = 'bit' | 'creditCard';\n\nconst paymentTitle: Record<PaymentType, string> = {\n  bit: texts.bit,\n  creditCard: texts.creditCard,\n};\n\nexport type PaymentData = {\n  text: string;\n  icon: React.ReactNode;\n  type: PaymentType;\n};\n\nexport function PaymentStep({\n  onError,\n  selectedAlertOption,\n  searchUrl,\n  onStepChange,\n}: StepWizardComponentProps) {\n  const iframeRef = useRef(null);\n\n  const [transactionId, setTransactionId] = useState<null | string>(null);\n\n  const [paymentIframe, setPaymentIframe] = useState('');\n\n  const [selectedPaymentType, setSelectedPaymentType] =\n    useState<PaymentType | null>(null);\n\n  useEventListener('message', (event) => {\n    if (event.data === 'paymentSuccess') {\n      onStepChange?.('purchase', {\n        PaymentMethod: snakeCase(selectedPaymentType || ''),\n        ecommerce: {\n          [snakeCase('transactionId')]: transactionId,\n          value: 79,\n          currency: 'ILS',\n          items: [\n            {\n              [snakeCase('itemCategory')]: 'alerts',\n              [snakeCase('itemName')]: 'immediate_update',\n              index: 0,\n              [snakeCase('itemVariant')]: 'new_alert',\n              price: 79,\n              quantity: 1,\n            },\n          ],\n        },\n      });\n\n      redirectToSuccessPage();\n    } else if (event.data === 'paymentFail') {\n      redirectToFailPage();\n    }\n  });\n\n  const getPaymentIframe = async (paymentType: PaymentType) => {\n    try {\n      setSelectedPaymentType(paymentType);\n      const { data } = await alerts.createPayment({\n        type: selectedAlertOption as AlertType,\n        url: searchUrl,\n        paymentSuccessUrl: `${window.location.origin}${TRANZILA_SUCCESS_URL}`,\n        paymentFailUrl: `${window.location.origin}${TRANZILA_FAIL_URL}`,\n      });\n\n      const iframe = data.data?.[paymentType];\n\n      if (!iframe) {\n        throw new Error('no iframe returned from server');\n   