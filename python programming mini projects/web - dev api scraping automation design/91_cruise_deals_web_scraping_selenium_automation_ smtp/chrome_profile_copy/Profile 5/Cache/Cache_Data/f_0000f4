n);\n\n  return titles.length > 0\n    ? transformCarFamilyType(titles)\n    : SUBCATEGORIES_TITLES_MAP[subcategory];\n};\n\nexport const getVehiclesTags = (feedLiteralData: FeedLiteralData): string[] => {\n  const {\n    manufacturer,\n    model,\n    subModel,\n    year,\n    price,\n    topArea,\n    area,\n    km,\n    hand,\n    engineType,\n    gearBox,\n    group_color: groupColor,\n    seats,\n    carTag: carTags = [],\n    electricRange,\n    batteryCapacity,\n  } = feedLiteralData || {};\n\n  const isElectricTagsExcluded =\n    engineType?.some(({ id }) => id === ELECTRIC_ENGINE_TYPE) &&\n    carTags.some(({ id }) => id === ELECTRIC_CAR_TAG);\n\n  const selectedCarTags = isElectricTagsExcluded\n    ? carTags.filter(({ id }) => id !== ELECTRIC_CAR_TAG)\n    : carTags;\n\n  const tagsWithoutLuxury = selectedCarTags?.filter(\n    ({ id }) => id !== LUXURY_CAR_TAG,\n  );\n\n  return Object.values({\n    ...(manufacturer && {\n      manufacturer: transformTitles(manufacturer),\n    }),\n    ...(model && {\n      model: transformTitles(model),\n    }),\n    ...(subModel && {\n      model: transformTitles(subModel),\n    }),\n    ...(year && {\n      year: transformYear(year),\n    }),\n    ...(price && {\n      price: transformPrice(price),\n    }),\n    ...(topArea && {\n      topArea: transformArea(topArea),\n    }),\n    ...(area && {\n      area: transformArea(area),\n    }),\n    ...(km && {\n      km: transformKM(km),\n    }),\n    ...(hand && {\n      hand: transformHand(hand),\n    }),\n    ...(engineType && {\n      engineType: transformTitles(engineType),\n    }),\n    ...(gearBox && {\n      gearBox: transformTitles(gearBox),\n    }),\n    ...(groupColor && {\n      groupColor: transformTitles(groupColor),\n    }),\n    ...(seats && {\n      seats: transformSeats(seats),\n    }),\n    ...(carTags && {\n      carTag: transformTitles(tagsWithoutLuxury),\n    }),\n    ...(electricRange && {\n      electricRange: transformElectricRange(electricRange),\n    }),\n    ...(batteryCapacity && {\n      batteryCapacity: transformBatteryCapacity(batteryCapacity),\n    }),\n  }).filter(Boolean);\n};\n","import type { FeedLiteralData } from '@y2/api-clients/feed-literal';\nimport { categories } from '@y2/categories';\nimport { RealEstateIcon, VehicleIcon } from '@y2/mango/components/icons';\n\nimport { getRealestateTags, getRealestateTitle } from './realestate-utils';\nimport { getVehiclesTags, getVehiclesTitle } from './vehicles-utils';\n\nexport const VEHICLES_CATEGORY = categories.vehicles.title;\n\nexport const REALESTATE_CATEGORY = categories.realEstate.title;\nexport type Category = typeof VEHICLES_CATEGORY | typeof REALESTATE_CATEGORY;\n\nexport type VehiclesSubcategory =\n  | typeof categories.vehicles.subCategories.cars.title\n  | typeof categories.vehicles.subCategories.motorcycles.title\n  | typeof categories.vehicles.subCategories.scooters.title;\n\nexport type RealestateSubcategory =\n  | typeof categories.realEstate.subCategories.forSale.title\n  | typeof categories.realEstate.subCategories.rent.title;\n\nexport type Subcategory = VehiclesSubcategory | RealestateSubcategory;\n\nexport type RangeSelection = {\n  from?: number;\n  to?: number;\n};\n\nexport const TITLE_FUNCTIONS: Record<\n  Category,\n  (subcategory: Subcategory, feedLiteralData: FeedLiteralData) => string\n> = {\n  [VEHICLES_CATEGORY]: (\n    subcategory: Subcategory,\n    feedLiteralData: FeedLiteralData,\n  ) => getVehiclesTitle(subcategory as VehiclesSubcategory, feedLiteralData),\n  [REALESTATE_CATEGORY]: (\n    subcategory: Subcategory,\n    feedLiteralData: FeedLiteralData,\n  ) =>\n    getRealestateTitle(subcategory as RealestateSubcategory, feedLiteralData),\n};\n\nexport const TAGS_FUNCTIONS: Record<\n  Category,\n  (feedLiteralData: FeedLiteralData) => string[]\n> = {\n  [VEHICLES_CATEGORY]: getVehiclesTags,\n  [REALESTATE_CATEGORY]: getRealestateTags,\n};\n\nexport const ICONS: Record<\n  Category,\n  React.MemoExoticComponent<\n    (props: React.SVGProps<SVGSVGElement>) => JSX.Element\n  >\n> = {\n  [VEHICLES_CATEGORY]: VehicleIcon,\n  [REALESTATE_CATEGORY]: RealEstateIcon,\n};\n\nexport const AGREEMENT_LEAD_TYPE = 59;\n","/* eslint-disable max-lines-per-function */\n\nimport type { NextApiRequest } from 'next';\nimport type { FeedLiteralData } from '@y2/api-clients/feed-literal';\n\nimport {\n  type Category,\n  ICONS,\n  type Subcategory,\n  TAGS_FUNCTIONS,\n  TITLE_FUNCTIONS,\n} from './consts';\nimport texts from './texts.json';\n\nexport const getCategory = (url: string): Category =>\n  url.split('/')[1] as Category;\n\nexport const getSubcategory = (url: string): Subcategory =>\n  url.split('/')[2] as Subcategory;\n\nexport const getQueryObject = (queryString: string) => {\n  const searchParams = new URLSearchParams(queryString);\n  const queryObject: NextApiRequest['query'] = {};\n  searchParams.forEach((value, key) => {\n    queryObject[key] = value;\n  });\n\n  return queryObject;\n};\n\nexport const getTitleByCategory = (\n  category: Category,\n  subcategory: Subcategory,\n  feedLiteralData: FeedLiteralData,\n) => TITLE_FUNCTIONS[category](subcategory, feedLiteralData);\n\nexport const getTagsByCategory = (\n  category: Category,\n  feedLiteralData: FeedLiteralData,\n) => TAGS_FUNCTIONS[category](feedLiteralData);\n\nexport const getIconByCategory = (category: Category) => ICONS[category];\n\nexport const transformTitles = (data: any[]) =>\n  data.map((item: any) => item.title).join(', ');\n\nexport const transformPrice = (price: any) => {\n  if (price.from && price.to) {\n    return price.from === price.to\n      ? ` ${price.from.toLocaleString()}`\n      : ` ${price.from.toLocaleString()} -   ${price.to.toLocaleString()}`;\n  }\n\n  if (price.from) {\n    return `${texts.from}  ${price.from.toLocaleString()}`;\n  }\n\n  if (price.to) {\n    return `${texts.to}  ${price.to.toLocaleString()}`;\n  }\n\n  return '';\n};\n","import type { NextApiRequest } from 'next';\nimport { useQuery,