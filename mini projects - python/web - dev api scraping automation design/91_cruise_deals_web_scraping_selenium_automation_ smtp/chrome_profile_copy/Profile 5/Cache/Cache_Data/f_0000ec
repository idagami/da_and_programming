, enforceSingleValue(key, value)] as const,\n  );\n\n  return {\n    errors: entries.map(([, value]) => value.error).filter(identity),\n    query: Object.fromEntries(\n      entries.map(([key, value]) => [key, value.value]),\n    ),\n  };\n}\n","export const DEAFULT_PAGE = 1;\n","import { API_QUERY_KEYS } from '@y2/api-clients/realestate-feed';\n\nimport type { SimpleQuery } from '../../enforce-simple-query';\nimport { DEAFULT_PAGE } from '../consts';\n\nexport const getPageUrl = (query: SimpleQuery): string =>\n  query[API_QUERY_KEYS.PAGE] || '';\n\nexport const decodePageUrl = (page: string): number =>\n  Number(page) || DEAFULT_PAGE;\n","import { type AriaRole, type ReactNode, useId, useRef } from 'react';\nimport { useKeyPress } from 'ahooks';\nimport cn from 'classnames';\n\nimport styles from './dropdown.module.scss';\nimport { Backdrop } from '../../ui/backdrop/backdrop';\n\ntype Props = {\n  toggleCollapse: () => void;\n  isCollapsed: boolean;\n  actionButton: ReactNode;\n  children: ReactNode;\n  className?: string;\n  isDefaultStyle?: boolean;\n  isFullWidth?: boolean;\n  role?: AriaRole;\n  isDisabled?: boolean;\n};\n\nexport const Dropdown = ({\n  toggleCollapse,\n  isCollapsed,\n  actionButton,\n  className,\n  children,\n  isFullWidth,\n  isDisabled,\n  role = 'dialog',\n  isDefaultStyle = true,\n}: Props) => {\n  const dropdownId = useId();\n  const actionButtonRef = useRef<HTMLButtonElement>(null);\n  const dropdownRef = useRef<HTMLDivElement>(null);\n\n  const handleEscapePress = () => {\n    if (isCollapsed) return;\n    toggleCollapse();\n    actionButtonRef.current?.focus();\n  };\n\n  useKeyPress('esc', handleEscapePress, { target: dropdownRef });\n\n  return (\n    <div\n      className={cn(styles.dropdownBox, isDisabled && styles.disabled)}\n      ref={dropdownRef}\n    >\n      <button\n        id={dropdownId}\n        ref={actionButtonRef}\n        disabled={isDisabled}\n        type=\"button\"\n        aria-haspopup=\"true\"\n        aria-e