t',\n    getRentDynamicSearchOptionsQueryKey(req),\n  ].filter(Boolean);\n\nconst getFetchMethod = async (req: DynamicSearchOptionsRequest) => {\n  const {\n    data: { data },\n  } = await client.getRentDynamicSearchOptions(req);\n\n  return data;\n};\n\nexport const getRentDynamicSearchOptionsQueryOptions = (\n  req: DynamicSearchOptionsRequest,\n): UseQueryOptions<RentDynamicSearchOptionsResponse> => ({\n  queryKey: getQueryKey(req),\n  queryFn: () => getFetchMethod(req),\n  useErrorBoundary: true,\n  enabled: false,\n});\n\nexport const useRentDynamicSearchOptions = (req: DynamicSearchOptionsRequest) =>\n  useQuery(getRentDynamicSearchOptionsQueryOptions(req));\n","/* eslint-disable max-lines */\n/* eslint-disable max-statements */\nimport { useEffect, useState } from 'react';\nimport { useRouter } from 'next/router';\nimport { useAsyncEffect } from 'ahooks';\nimport isEqual from 'lodash/isEqual';\nimport omit from 'lodash/omit';\nimport {\n  FEED_SORT_OPTIONS_MAP,\n  RentDynamicSearchOptionsResponse,\n} from '@y2/api-clients/realestate-search-options';\nimport { ExtraFilters } from '@y2/feed-search/features/extra-filters';\nimport { useCollapseHash } from '@y2/hooks/use-collapse-hash';\nimport { useDisableScrollOnMobile } from '@y2/hooks/use-disable-mobile-scroll';\nimport { useRouterHash } from '@y2/hooks/use-router-hash';\n\nimport styles from './rent-search-bar.module.scss';\nimport texts from './rent-search-bar.texts.json';\nimport { CreateAlertButton } from '../../../components/create-alert-button/create-alert-button';\nimport { SearchBar } from '../../../components/search-bar';\nimport { useSearchContext } from '../../../hooks/use-search/search-provider';\nimport { useRentBaseSearchOptions } from '../../../query-hooks/realestate-search-options/rent/base';\nimport { useRentDynamicSearchOptions } from '../../../query-hooks/realestate-search-options/rent/dynamic';\nimport {\n  CLIENT_QUERY_KEYS_MAP,\n  type SearchClientQuery,\n  SUBCATEGORIES,\n} from '../../../services/feed/params/consts';\nimport { enforceSimpleQuery } from '../../../services/feed/params/enforce-simple-query';\nimport { encodeRentQueryParams } from '../../../services/query-params-utils/encode-client-query';\nimport { pushSearchBarOnSubmitEvent } from '../../analytics/feed-events';\nimport { LatestSearchesVersionA } from '../../latest-searches/latest-searches-version-a/latest-searches-version-a';\nimport { useAbTesting } from '../../latest-searches/use-ab-testing';\nimport { HASH } from '../../map-page-layout/consts';\nimport { getBoundsByUserCoords } from '../../map-page-layout/mapbox/utils/get-bounds-by-user-coords';\nimport {\n  type AdAttributes,\n  AdAttributesInput,\n  DEFAULT_AD_ATTRIBUTES,\n} from '../ad-attributes-input';\nimport { AdvertiserInput, DEFAULT_ADVERTISER } from '../advertiser-input';\nimport { BuiltSquareMeterRangeInput } from '../built-squaremeter-range-input/built-squaremeter-range-input';\nimport { EntranceDateInput } from '../entrance-date-input';\nimport { FloorRangeInput } from '../floor-range-input/floor-range-input';\nimport { transformFloorSearchOption } from '../floor-range-input/helpers';\nimport { FreeTextInput } from '../free-text-input';\nimport { LocationInput } from '../location-input';\nimport { addLocationsSearchToStorage } from '../location-input/utils/local-storage';\nimport { PriceRangeInput } from '../price-range-input';\nimport { PropertyCharacteristicsInput } from '../property-characteristics-input';\nimport { ShelterTooltip } from '../property-characteristics-input/shelter-tooltip/shelter-tooltip';\nimport { useShelterTooltip } from '../property-characteristics-input/shelter-tooltip/use-shelter-tooltip/use-shelter-tooltip';\nimport { PropertyConditionInput } from '../property-condition-input';\nimport { PropertyTypesInput, updateOptions } from '../property-types-input';\nimport { getPropertyTypeGroups } from '../property-types-input/get-property-type-groups';\nimport { RoomRangeInput } from '../room-range-input';\nimport { RentSearchFormInputs } from '../search-bar-utils';\nimport { SquareMeterRangeInput } from '../squaremeter-range-input/squaremeter-range-input';\nimport { TransactionType } from '../transaction-type';\n\ntype Props = {\n  initialSearchFormInputs: RentSearchFormInputs;\n  shouldShowAlertButton: boolean;\n  mapDivRef: React.RefObject<HTMLDivElement>;\n};\n\nexport const RentSearchBar = ({\n  initialSearchFormInputs: {\n    priceRange: initialPriceRange,\n    propertyTypes: initialPropertyTypes,\n    squareMeterRange: initialSquareMeterRange,\n    squareMeterBuildRange: initialSquareMeterBuildRange,\n    floorRange: initialFloorRange,\n    roomRange: initialRoomRange,\n    adAttributes: initialAdAttributes,\n    advertiser: initialAdvertiser,\n    locations: initialLocations,\n    entranceDate: initialEntranceDate,\n    propertyCharacteristics: initialPropertyCharacteristics,\n    propertyCondition: initialPropertyCondition,\n    freeText: initialFreeText,\n    sort,\n    bBox,\n  },\n  shouldShowAlertButton,\n  mapDivRef,\n}: Props) => {\n  const router = useRouter();\n  const { query } = enforceSimpleQuery(router.query);\n  const currentHash = useRouterHash();\n  const hasMapHash = currentHash.includes(HASH.MAP);\n  const { setIsSearching } = useSearchContext();\n  const [showLatestSearches, setShowLatestSearches] = useState(false);\n  const { isAVariant } = useAbTesting();\n\n  useEffect(() => {\n    setShowLatestSearches(isAVariant);\n  }, []);\n\n  const [freeText, setFreeText] =\n    useState<RentSearchFormInputs['freeText']>(initialFreeText);\n\n  const { collapse, isCollapsed } = useCollapseHash(HASH.SEARCH);\n  useDisableScrollOnMobile(!isCollapsed);\n\n  const { data: baseSearchOptions } = useRentBaseSearchOptions();\n\n  const [isNearMeSelected, setIsNearMeSelected] = useState(\n    !!query[CLIENT_QUERY_KEYS_MAP.NEAR_ME],\n  );\n\n  const [propertyTypes, setPropertyTypes] =\n    useState<RentSearchFormInputs['propertyTypes']>(initialPropertyTypes);\n\n  const [priceRange, setPriceRange] =\n    useState<RentSearchFormInputs['priceRange']>(initialPriceRange);\n  const [squareMeterRange, setSquareMeterRange] = useState<\n    RentSearchFormInputs['squareMeterRange']\n  >(initialSquareMeterRange);\n  const [squareMeterBuildRange, setSquareMeterBuildRange] = useState<\n    RentSearchFormInputs['squareMeterRange']\n  >(initialSquareMeterBuildRange);\n  const [floorRange, setFloorRange] =\n    useState<RentSearchFormInputs['floorRange']>(initialFloorRange);\n  const [roomRange, setRoomRange] =\n    useState<RentSearchFormInputs['roomRange']>(initialRoomRange);\n\n  const [propertyCondition, setPropertyCondition] = useState<\n    RentSearchFormInputs['propertyCondition']\n  >(initialPropertyCondition);\n\n  const [adAttributes, setAdAttributes] =\n    useState<RentSearchFormInputs['adAttributes']>(initialAdAttributes);\n\n  const [advertiser, setAdvertiser] =\n    useState<RentSearchFormInputs['advertiser']>(initialAdvertiser);\n\n  const [locations, setLocations] =\n    useState<RentSearchFormInputs['locations']>(initialLocations);\n\n  const [entranceDate, setEntranceDate] =\n    useState<RentSearchFormInputs['entranceDate']>(initialEntranceDate);\n\n  const propertyTypesValues = propertyTypes\n    .filter(({ checked }) => checked)\n    .map(({ value }) => value)\n    .join();\n\n  const { refetch: refetchForsaleDynamicSearchOptions } =\n    useRentDynamicSearchOptions({\n      ...(propertyTypesValues && {\n        [CLIENT_QUERY_KEYS_MAP.PROPERTY]: propertyTypesValues,\n      }),\n    });\n\n  const [dynamicSearchOptions, setDynamicSearchOptions] =\n    useState<RentDynamicSearchOptionsResponse>({});\n\n  useAsyncEffect(async () => {\n    const { data: dynamicSearchOptions } =\n      await refetchForsaleDynamicSearchOptions();\n\n    if (!dynamicSearchOptions) return;\n\n    setDynamicSearchOptions(dynamicSearchOptions);\n  }, [propertyTypesValues]);\n\n  const [propertyCharacteristics, setPropertyCharacteristics] = useState<\n    RentSearchFormInputs['propertyCharacteristics']\n  >(initialPropertyCharacteristics);\n\n  const getUpdatedSearchClientQuery = (): SearchClientQuery => {\n    const encodedParams = encodeRentQueryParams(\n      {\n        locations,\n        propertyTypes,\n        roomRange,\n        priceRange,\n        squareMeterRange,\n        squareMeterBuildRange,\n        propertyCondition,\n        floorRange,\n        adAttributes,\n        advertiser,\n        propertyCharacteristics,\n        sort,\n        entranceDate,\n        freeText,\n        ...(locations.length === 0 && {\n          bBox: isNearMeSelected ? getBoundsByUserCoords(mapDivRef) : bBox,\n        }),\n      },\n      baseSearchOptions,\n    );\n\n    return {\n      ...encodedParams,\n      ...(isNearMeSelected && {\n        [CLIENT_QUERY_KEYS_MAP.NEAR_ME]: '1',\n        [CLIENT_QUERY_KEYS_MAP.SORT]: FEED_SORT_OPTIONS_MAP.NEAR_ME,\n      }),\n    };\n  };\n\n  const handleSubmit = () => {\n    const updatedSearchClientQuery = getUpdatedSearchClientQuery();\n    if (!isEqual(query, updatedSearchClientQuery)) {\n      pushSearchBarOnSubmitEvent();\n      // eslint-disable-next-line custom-rules/no-nested-if-conditions\n      if (locations.length > 0) {\n        addLocationsSearchToStorage(locations);\n      }\n      router.push({\n        query: updatedSearchClientQuery,\n        ...(hasMapHash && { hash: HASH.MAP }),\n      });\n    } else {\n      collapse();\n    }\n    setIsSearching(true);\n  };\n\n  const resetExtraFilters = () => {\n    setAdAttributes(DEFAULT_AD_ATTRIBUTES);\n    setAdvertiser(DEFAULT_ADVERTISER);\n    setPropertyCharacteristics([]);\n    setEntranceDate([null, null]);\n    setPropertyCondition([]);\n    setFreeText('');\n    setSquareMeterRange([\n      baseSearchOptions.squaremeter.from,\n      baseSearchOptions.squaremeter.to,\n    ]);\n    setSquareMeterBuildRange([\n      baseSearchOptions.squareMeterBuild.from,\n      baseSearchOptions.squareMeterBuild.to,\n    ]);\n    setFloorRange([baseSearchOptions.floor.from, baseSearchOptions.floor.to]);\n  };\n\n  const getActiveExtraFiltersCount = () => {\n    const isAdAttributesFilterActive = Object.keys(adAttributes).some(\n      (adAttributeKey) => adAttributes[adAttributeKey as keyof AdAttributes],\n    );\n    const isAdvertiserFilterActive = Object.keys(advertiser).some(\n      (advertiserKey) => advertiser[advertiserKey as keyof AdvertiserInput],\n    );\n    const isPropertyConditionFilterActive = propertyCondition.length > 0;\n\n    const isPropertyCharacteristicsFilterActive =\n      propertyCharacteristics.length > 0;\n    const isEntranceDateActive = entranceDate.some(Boolean);\n    const isSquareMeterRangeActive = !isEqual(squareMeterRange, [\n      baseSearchOptions.squaremeter.from,\n      baseSearchOptions.squaremeter.to,\n    ]);\n    const isSquareMeterBuildRangeActive = !isEqual(squareMeterBuildRange, [\n      baseSearchOptions.squareMeterBuild.from,\n      baseSearchOptions.squareMeterBuild.to,\n    ]);\n\n    const isFloorRangeActive = !isEqual(floorRange, [\n      baseSearchOptions.floor.from,\n      baseSearchOptions.floor.to,\n    ]);\n\n    return [\n      freeText,\n      isAdAttributesFilterActive,\n      isAdvertiserFilterActive,\n      isPropertyCharacteristicsFilterActive,\n      isFloorRangeActive,\n      isEntranceDateActive,\n      isSquareMeterRangeActive,\n      isSquareMeterBuildRangeActive,\n      isPropertyConditionFilterActive,\n    ].filter(Boolean).length;\n  };\n\n  const filters = omit(getUpdatedSearchClientQuery(), [\n    CLIENT_QUERY_KEYS_MAP.FOR_PARTNERS,\n    CLIENT_QUERY_KEYS_MAP.PETS,\n    CLIENT_QUERY_KEYS_MAP.MIN_PRICE,\n    CLIENT_QUERY_KEYS_MAP.MAX_PRICE,\n  ]);\n\n  const { shouldShowShelterTooltip, closeShelterTooltip } = useShelterTooltip();\n\n  return (\n    <>\n      <SearchBar\n        hash={HASH.SEARCH}\n        subcategory={SUBCATEGORIES.RENT}\n        onSubmit={handleSubmit}\n        collapse={collapse}\n   