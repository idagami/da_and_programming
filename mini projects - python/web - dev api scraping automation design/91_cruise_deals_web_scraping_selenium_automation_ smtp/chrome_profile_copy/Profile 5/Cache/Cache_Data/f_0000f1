mber) => string;\n  const getOptionClass: GetOptionClass = (option) => {\n    if (isNoRangeSelected) {\n      return '';\n    }\n    if (isSingleValueSelected && option === startRange) {\n      return styles.selected;\n    }\n    if (isBothRangesSelected) {\n      if (option === startRange) return styles.start;\n      if (option < endRange && option > startRange) return styles.inRange;\n      if (option === endRange) return styles.end;\n    }\n\n    return '';\n  };\n  const formatButtonRangeText = (option: number, index: number) => {\n    const isLastOption = index === options.length - 1;\n\n    return isLastOption ? `+${option}` : option;\n  };\n  const handleChangeRange = (selectedOption: number) => {\n    if (selectedOption === startRange || selectedOption === endRange) {\n      // Clear selection\n      setValue([null, null]);\n    } else if (isNoRangeSelected || isBothRangesSelected) {\n      // Select single value\n      setValue([selectedOption, selectedOption]);\n    } else {\n      // Select range values\n      setValue([\n        Math.min(startRange as number, selectedOption),\n        Math.max(startRange as number, selectedOption),\n      ]);\n    }\n  };\n\n  return (\n    <div className={styles.buttonsRangeBox}>\n      <Swiper\n        className={styles.buttonsSwiper}\n        slidesPerView=\"auto\"\n        slidesOffsetBefore={20}\n        slidesOffsetAfter={20}\n        breakpoints={{\n          880: {\n            allowTouchMove: false,\n          },\n        }}\n      >\n        {options.map((option, index) => (\n          <SwiperSlide className={styles.slide} key={option}>\n            <span className={cn(styles.buttonBox, getOptionClass(option))}>\n              <button\n                type=\"button\"\n                onClick={() => handleChangeRange(option)}\n                className={styles.button}\n                data-nagish=\"buttons-range-button\"\n                data-testid={`realestate-room-range-option-${option}`}\n              >\n                {formatButtonRangeText(option, index)}\n              </button>\n            </span>\n          </SwiperSlide>\n        ))}\n      </Swiper>\n    </div>\n  );\n};\n","import { type Dispatch, type SetStateAction, useEffect, useState } from 'react';\nimport isNumber from 'lodash/isNumber';\nimport range from 'lodash/range';\nimport type {\n  Range,\n  RangeOptions,\n} from '@y2/api-clients/realestate-search-options';\nimport { RoundedPseudoDropButton } from '@y2/feed-search/components//ui/pseudo-drop-button';\nimport { ButtonsRange } from '@y2/feed-search/components/composed/buttons-range';\nimport { ResponsiveDropdown } from '@y2/feed-search/components/composed/responsive-dropdown';\nimport { InputTitle } from '@y2/feed-search/components/ui/input-title';\nimport { useCollapse } from '@y2/hooks/use-collapse';\n\nimport styles from './room-range-input.module.scss';\nimport texts from './texts.json';\n\ntype Props = {\n  value: Range;\n  onChange: Dispatch<SetStateAction<Range>>;\n  options: RangeOptions;\n  disabled?: boolean;\n  showSelectedAsInitials?: boolean;\n  onSelectedClassName?: string;\n};\n\nexport const RoomRangeInput = ({\n  value,\n  onChange,\n  showSelectedAsInitials,\n  onSelectedClassName,\n  options,\n  disabled,\n}: Props) => {\n  const { isCollapsed, toggleCollapse, collapseRef } = useCollapse();\n\n  const [minValue, maxValue] = value;\n\n  const areBothLimitsSelected =\n    minValue === options.from && maxValue === options.to;\n\n  const [selectedRange, setSelectedRange] = useState<Range>(\n    areBothLimitsSelected ? [null, null] : value,\n  );\n\n  const handleSelection = (range: Range) => {\n    setSelectedRange(range);\n    onChange(range);\n  };\n\n  const minRooms = areBothLimitsSelected ? null : minValue;\n  const maxRooms = areBothLimitsSelected ? null : maxValue;\n\n  const isMinRoomsRangeSelected = isNumber(minRooms);\n\n  const isMaxRoomsRangeSelected = isNumber(maxRooms);\n\n  const isRoomsRangeSelected =\n    isMinRoomsRangeSelected || isMaxRoomsRangeSelected;\n\n  const roomsLastOptionValue = options.to;\n  const roomsOptions = range(\n    options.from,\n    options.to + options.step,\n    options.step,\n  );\n\n  const getRoomDisplayText = (selectedRange: Range) => {\n    const [minRooms, maxRooms] = selectedRange;\n    if (!minRooms && !maxRooms) {\n      return texts.rooms;\n    }\n    const minRoomsText = `${minRooms}${minRooms === roomsLastOptionValue ? '+' : ''}`;\n    const shouldShowMaxRoomsText = maxRooms && maxRooms !== minRooms;\n    const maxRoomsText =\n      shouldShowMaxRoomsText &&\n      `- ${maxRooms}${maxRooms === roomsLastOptionValue ? '+' : ''}`;\n\n    const roomsDescriptiveText =\n      showSelectedAsInitials && minRooms !== maxRooms\n        ? texts.roomsInitial\n        : texts.rooms;\n\n    return [minRoomsText, maxRoomsText, roomsDescriptiveText]\n      .filter(Boolean)\n      .join(' ');\n  };\n\n  const [roomsDisplayText, setRoomsDisplayText] = useState(\n    getRoomDisplayText(selectedRange),\n  );\n\n  useEffect(() => {\n    setRoomsDisplayText(getRoomDisplayText(selectedRange));\n  }, [selectedRange]);\n\n  return (\n    <div ref={collapseRef} data-testid=\"room-range-input\">\n      <ResponsiveDropdown\n        actionButton={\n          <RoundedPseudoDropButton\n            isOpen={!isCollapsed}\n            activeClassName={onSelectedClassName}\n            isDisabled={disabled}\n            isActive={isRoomsRangeSelected}\n            data-testid=\"realestate-filter-rooms-button\"\n          >\n            {roomsDisplayText}\n          </RoundedPseudoDropButton>\n        }\n        isCollapsed={isCollapsed}\n        toggleCollapse={toggleCollapse}\n        className={styles.roomsDropdownBox}\n      >\n        <InputTitle title={texts.rooms} className={styles.label} onlyOnMobile />\n        <ButtonsRange\n          value={selectedRange}\n          setValue={handleSelection}\n          options={roomsOptions}\n        />\n      </ResponsiveDropdown