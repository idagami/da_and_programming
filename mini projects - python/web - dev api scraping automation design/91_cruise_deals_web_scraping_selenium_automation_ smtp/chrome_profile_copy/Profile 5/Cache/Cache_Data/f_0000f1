 { useEffect, useMemo, useState } from 'react';\nimport cn from 'classnames';\n\nimport styles from './select-range.module.scss';\nimport { MinusIcon } from '../../../components/icons';\nimport type {\n  FieldProps as MenuFieldProps,\n  MenuItem,\n} from '../../dropdown/types';\nimport {\n  BaseSelect,\n  BaseSelectProps,\n  SelectArgs,\n  Value,\n} from '../../select/base';\n\nexport type Item = Omit<MenuItem, 'value'> & { value: number };\n\ntype FieldProps = Pick<MenuFieldProps, 'maxOptionsInView'>;\n\ntype RangeValues = {\n  from: number | null;\n  to: number | null;\n};\n\ntype ChangeEvent = (value: RangeValues, eventArgs?: SelectArgs) => void;\n\ntype NativeDivProps = React.DetailedHTMLProps<\n  React.HTMLAttributes<HTMLDivElement>,\n  HTMLDivElement\n>;\n\nexport type SelectRangeProps = Omit<NativeDivProps, 'onChange'> &\n  FieldProps & {\n    values: RangeValues;\n    onChange: ChangeEvent;\n    name: string;\n    options: Item[];\n    fromLabel?: string;\n    toLabel?: string;\n    fromPlaceholders?: string;\n    toPlaceholders?: string;\n    className?: string;\n    fromTopRowArgs?: BaseSelectProps['topRowArgs'];\n    toTopRowArgs?: BaseSelectProps['topRowArgs'];\n    disabled?: boolean;\n    headerSlots?: {\n      from?: BaseSelectProps['headerSlot'];\n      to?: BaseSelectProps['headerSlot'];\n    };\n  };\n\nconst parseValueOnSelect = (value: Value): number | null => {\n  const parsedVal = parseInt(value as string);\n\n  return !isNaN(parsedVal) ? parsedVal : null;\n};\n\nexport const SelectRange = ({\n  onChange,\n  values,\n  className,\n  name,\n  options,\n  fromLabel = '',\n  toLabel = '',\n  fromPlaceholders = '',\n  toPlaceholders = '',\n  fromTopRowArgs,\n  toTopRowArgs,\n  headerSlots,\n  maxOptionsInView,\n  disabled,\n  ...otherHtmlAttributes\n}: SelectRangeProps) => {\n  const [fromOptions, setFromOptions] = useState<Item[]>([]);\n  const [toOptions, setToOptions] = useState<Item[]>([]);\n  const hasLabels = !!fromLabel || !!toLabel;\n\n  useMemo(() => {\n    setFromOptions(options);\n    setToOptions(options);\n    // eslint-disable-next-line react-hooks/exhaustive-deps\n  }, [options]);\n\n  useEffect(() => {\n    const { from, to } = values;\n    setToOptions(\n      options.filter((option) => from === null || option.value >= from),\n    );\n    setFromOptions(\n      options.filter((option) => to === null || option.value <= to),\n    );\n    // eslint-disable-next-line react-hooks/exhaustive-deps\n  }, [values]);\n\n  return (\n    <div\n      data-testid=\"select-range\"\n      className={cn(styles.box, className)}\n      {...otherHtmlAttributes}\n    >\n      <BaseSelect\n        name={`${name}-from`}\n        value={values.from !== null ? values.from : ''}\n        onChange={(value, eventOptions) =>\n          onChange({ ...values, from: parseValueOnSelect(value) }, eventOptions)\n        }\n        placeholder={fromPlaceholders}\n        label={fromLabel}\n        list={fromOptions}\n        headerSlot={headerSlots?.from}\n        maxOptionsInView={maxOptionsInView}\n        disabled={disabled}\n        topRowArgs={\n          fromTopRowArgs\n            ? fromTopRowArgs\n            : {\n                mobileTitle: fromLabel,\n                isMobileOnly: true,\n              }\n        }\n      />\n      <span className={cn(styles.divider, hasLabels && styles.hasLabels)}>\n        <MinusIcon />\n      </span>\n      <BaseSelect\n        name={`${name}-to`}\n        value={values.to !== null ? values.to : ''}\n        onChange={(value, eventOptions) =>\n          onChange({ ...values, to: parseValueOnSelect(value) }, eventOptions)\n        }\n        placeholder={toPlaceholders}\n        label={toLabel}\n        list={toOptions}\n        headerSlot={headerSlots?.to}\n        maxOptionsInView={maxOptionsInView}\n        disabled={disabled}\n        topRowArgs={\n          toTopRowArgs\n            ? toTopRowArgs\n            : {\n                mobileTitle: toLabel,\n                isMobileOnly: true,\n              }\n        }\n      />\n    </div>\n  );\n};\n","import { useEffect, useState } from 'react';\n\nimport { Item, SelectRange, SelectRangeProps } from './base';\n\nexport type OpenSelectRangeProps = Omit<SelectRangeProps, 'options'> & {\n  min: number;\n  max: number;\n  step: number;\n};\n\nconst createOptionsFromRange = (\n  min: number,\n  max: number,\n  step: number,\n): Item[] => {\n  let current = min;\n  const options = [];\n  while (current <= max) {\n    options.push(current);\n    current = current + step;\n  }\n\n  return options.map((option) => ({ text: String(option), value: option }));\n};\n\nexport const OpenSelectRange = ({\n  min,\n  max,\n  step,\n  ...rangeProps\n}: OpenSelectRangeProps) => {\n  const [options, setOptions] = useState<Item[]>([]);\n\n  useEffect(() => {\n    setOptions(createOptionsFromRange(min, max, step));\n    // eslint-disable-next-line react-hooks/exhaustive-deps\n  }, [min, max, step]);\n\n  return (\n    <SelectRange\n      {...rangeProps}\n      options={options}\n      data-testid=\"open-select-range\"\n    />\n  );\n};\n","export * from './base';\nexport * from './open-select-range';\n","export const NIXALE_SENT_EVENTS_KEY = 'nixaleEventKeys';\n\nexport const AD_VIEW_EVENT = 'view';\nexport const RECOMMENDATION_AD_VIEW_EVENT = 'viewReco';\nexport const CONTACT_SELLER_EVENT = 'buy';\n\nexport const RECOMMENDATION_TYPE = 'recommendation-type';\nexport const RECOMMENDATION_CAROUSEL_INSTANCE =\n  'recommendation-carousel-instance';\nexport const DEFAULT_CAROUSEL_INSTANCE = 1;\n","import {\n  RecommendationsApi,\n  recommendationsClient,\n} from '@y2/api-clients/recommendations';\nimport { log } from '@y2/log';\n\nconst recommendations = recommendationsClient();\n\nexport const sendEvent = async (\n  reqBody: RecommendationsApi.EventRequestBody,\n): Promise<Record<string, string>> => {\n  try {\n    const {\n      data: { data },\n    } = await recommendations.post(reqBody);\n\n    return data;\n  } 