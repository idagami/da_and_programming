pubads().refresh([adSlot]);\n      // eslint-disable-next-line custom-rules/no-nested-if-conditions\n      if (hasDebugCookie)\n        log.info(`the ad ${adSlot.getSlotElementId()} was refreshed`);\n      refreshesPerformed++;\n    } else {\n      clearInterval(refreshTimer);\n    }\n  }, refreshInterval);\n\n  return refreshTimer;\n};\n\nexport const defineAndDisplayAdSlot = (\n  networkId: string,\n  device: Device,\n  path: string,\n  elementId: string,\n  size: SlotSize,\n) => {\n  const slot =\n    window.googletag\n      .defineSlot(\n        `${networkId}/${device}/${path}/${elementId}`,\n        size,\n        elementId,\n      )\n      .addService(window.googletag.pubads()) ?? null;\n  window.googletag.display(elementId);\n\n  return slot;\n};\n\nexport const destroyDfpSlot = (\n  slot: GoogletagSlot | null,\n  refreshTimer: NodeJS.Timeout | null,\n  onSlotRenderEnded: SlotRenderEndedHandler,\n) => {\n  if (refreshTimer) {\n    clearInterval(refreshTimer);\n  }\n  window.googletag.cmd.push(() => {\n    window.googletag\n      .pubads()\n      .removeEventListener('slotRenderEnded', onSlotRenderEnded);\n    if (slot) {\n      window.googletag.destroySlots([slot]);\n    }\n  });\n};\n\nexport function getDeviceClassName(device: Device) {\n  return device === 'desktop' ? styles.desktopOnly : styles.mobileOnly;\n}\n\nexport const buildAdUnitPath = (\n  catId: number,\n  subCatId?: number,\n  subSubCatId?: number,\n): string => {\n  const catName = CATEGORIES[catId];\n  if (!catName) {\n    throw new Error(`Category ID ${catId} not found.`);\n  }\n  if (!subCatId) {\n    return catName;\n  }\n  const path: string[] = [catName];\n\n  const subCatMapping = SUB_CATEGORIES[catName];\n  if (!subCatMapping || !subCatMapping[subCatId]) {\n    throw new Error(\n      `Subcategory ID ${subCatId} not found in category ${catName}.`,\n    );\n  }\n\n  const isFashionSubCat = catId === SECOND_HAND && subCatId === FASHION;\n  let subCatName = subCatMapping[subCatId];\n  if (